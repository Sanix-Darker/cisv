{
  "name": "cisv",
  "version": "0.0.7",
  "description": "The csv parser on steroids.",
  "author": "sanix<s4nixd@gmail.com>",
  "main": "./build/Release/cisv.node",
  "types": "./types/cisv.d.ts",
  "exports": {
    ".": {
      "import": "./cisv/index.mjs",
      "require": "./cisv/index.js"
    }
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "build": "node-gyp rebuild",
    "test": "mocha ./tests/*.test.js && bash ./test_transform.sh",
    "test:build": "npm run build && npm run test",
    "benchmark": "node benchmark/benchmark.js",
    "lint": "clang-format -i cisv/*.{cc,h}",
    "prepublishOnly": "npm run benchmark",
    "pretest:typescript": "tsc --project tsconfig.json --noEmit",
    "build:types": "tsc --project tsconfig.json --declaration --emitDeclarationOnly",
    "test:all": "npm run test && npm run test:typescript",
    "test:coverage": "nyc mocha test/*.test.js",
    "test:perf": "node test/performance.test.js"
  },
  "dependencies": {
    "node-addon-api": "^5.0.0",
    "node-gyp-build": "^4.8.4",
    "udsv": "^0.7.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.9",
    "benchmark": "^2.1.4",
    "chai": "^4.3.6",
    "csv-parse": "^6.0.0",
    "fast-csv": "^5.0.2",
    "mocha": "^10.0.0",
    "neat-csv": "^7.0.0",
    "nyc": "^15.0.0",
    "papaparse": "^5.5.3",
    "ts-mocha": "^10.1.0",
    "typescript": "^5.8.3"
  },
  "gypfile": true,
  "keywords": [
    "csv",
    "parser",
    "fast",
    "simd",
    "native"
  ],
  "license": "MIT"
}
